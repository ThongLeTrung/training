<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui" xmlns:f="urn:import:in.fins.client.widget">

	<ui:with type="in.fins.client.FinsResources" field="resource" />

	<g:StackLayoutPanel unit="EM">
		<g:stack>
			<g:customHeader size='1.8'>
				<f:MenuHeader text="Analyze" imageResource="{resource.home2}" />
			</g:customHeader>
			<g:LayoutPanel>
				<g:layer left='0.5em' width='10em' top='0.5em' height='3em'>
					<f:MenuItem contentName="Snapshot" text="Symbol" imageResource="{resource.home3}" />
				</g:layer>
				<!-- top increment by 1.8 -->
				<g:layer left='0.5em' width='10em' top='2.3em' height='3em'>
					<f:MenuItem contentName="Watchlist" text="Watchlists" imageResource="{resource.home4}" />
				</g:layer>
			</g:LayoutPanel>
		</g:stack>		
		<g:stack>
			<g:customHeader size='1.8'>
				<f:MenuHeader text="Manage" imageResource="{resource.home5}" />
			</g:customHeader>
			<g:LayoutPanel>
				<g:layer left='0.5em' width='10em' top='0.5em' height='3em'>
					<f:MenuItem contentName="ManageWatchlist" text="Watchlist" imageResource="{resource.home6}" />
				</g:layer>
			</g:LayoutPanel>
		</g:stack>
	</g:StackLayoutPanel>

</ui:UiBinder>